{
  "expo": {
    "privacy": "public",
    "name": "Dozan",
    "slug": "Dozan",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "bundleIdentifier": "com.hobaexpoaccount.dozan",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/adaptive-icon-background.png",
        "backgroundColor": "#cddae2"
      },
      "package": "com.hobaexpoaccount.dozan",
      "versionCode": 3,
      "runtimeVersion": "3.0.0",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",

          "resizeMode": "contain",

          "backgroundColor": "#cddae2"
        }
      ],
      "expo-asset",
      [
        "expo-custom-assets",
        {
          "assetsPaths": ["./assets/sounds"],
          "preserveFolder": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9f5e5310-0b79-4901-9a67-21e70b83a2ac"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/9f5e5310-0b79-4901-9a67-21e70b83a2ac"
    }
  }
}

{
  "expo": {
    "name": "Dozan",
    "slug": "Dozan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "bundleIdentifier": "com.hobaexpoaccount.eartrainingapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#652eb6"
      },
      "package": "com.hobaexpoaccount.eartrainingapp",
      "versionCode": 1,
      "runtimeVersion": "1.0.0",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#652eb6"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2132aa56-1928-4348-aaa0-57aebfff8a1d"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2132aa56-1928-4348-aaa0-57aebfff8a1d"
    }
  }
}
